<form action='/links/new' method='get'>
  <input value='Add bookmark' type='submit'>
</form>

<ul id='links'>
  <% @links.each do |link| %>
    <li>
      <%= link.title %> <%= link.url %>
      tags:
      <% @tags.each do |tag| %>
        <%= tag.name %>
      <% end %>
    </li>
    <% end %>
</ul>
